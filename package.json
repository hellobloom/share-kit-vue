{
  "name": "@bloomprotocol/share-kit-vue",
  "version": "1.0.0",
  "description": "Vue wrapper for @bloomprotocol/share-kit",
  "main": "dist/index.js",
  "module": "dist/share-kit-vue.esm.js",
  "typings": "dist/index.d.ts",
  "repository": "https://github.com/hellobloom/share-kit-vue",
  "author": "Bloom Team <team@bloom.co>",
  "license": "Apache-2.0",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "npm run clean && tsdx watch --tsconfig ./tsconfig.build.json",
    "build": "npm run clean && tsdx build --tsconfig ./tsconfig.build.json",
    "clean": "rimraf dist",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test && npm run eslint",
    "test": "tsdx test --env=jsdom",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "eslint": "./bin/eslint",
    "eslint-fix": "./bin/eslint --fix"
  },
  "dependencies": {
    "@bloomprotocol/share-kit": "^7.0.0"
  },
  "peerDependencies": {
    "vue": "^2.6.10",
    "vue-property-decorator": "^8.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@storybook/vue": "^5.1.9",
    "@types/jest": "^24.0.15",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "@typescript-eslint/parser": "^1.7.0",
    "@vue/babel-preset-app": "^3.7.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "acorn": "^6.1.1",
    "babel-loader": "^8.0.5",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^5.0.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "jest-canvas-mock": "^2.0.0-alpha.1",
    "jest-resolve": "^24.8.0",
    "prettier": "^1.18.0",
    "rimraf": "^2.6.2",
    "ts-loader": "^5.4.4",
    "tsdx": "^0.7.2",
    "tslib": "^1.10.0",
    "typescript": "^3.5.2",
    "vue": "^2.6.10",
    "vue-class-component": "^7.0.1",
    "vue-fragment": "^1.5.1",
    "vue-loader": "^15.7.0",
    "vue-property-decorator": "^8.1.0",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.30.0"
  },
  "jest": {
    "setupFiles": [
      "jest-canvas-mock"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
